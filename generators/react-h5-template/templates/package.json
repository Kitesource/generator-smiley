{
  "name": "react_h5_template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build-test": "tsc -b && vite build --mode test",
    "build-gzip": "tsc -b && vite build --mode production.gizp",
    "build-noCache": "pnpm clean-cache && pnpm build",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "lint-prettier": "prettier --write .",
    "lint-eslint": "eslint . --fix",
    "lint-style": "stylelint src/**/*.{vue,css,scss} --fix",
    "lint": "pnpm lint-eslint && pnpm lint-prettier && pnpm lint-style",
    "preview": "vite preview",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add ."
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write",
      "git add ."
    ]
  },
  "dependencies": {
    "antd-mobile": "^5.41.1",
    "antd-mobile-icons": "^0.3.0",
    "axios": "^1.13.2",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash-es": "^4.17.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.5",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.94.2",
    "stylelint": "^16.26.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "unplugin-auto-import": "^20.2.0",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
