{
  "name": "<%= name || 'project-name'%>",
  "homepage": "",
  "private": false,
  "version": "1.0.0",
  "contributors": [],
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --fix"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "scripts": {
    "cz": "git add . && cz",
    "serve": "vite --mode localhost",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "format": "prettier --write .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.json",
    "preview": "vite preview",
    "prepare": "husky install",
    "i18n:extract": "vue-i18n-extract report --vueFiles './src/**/*.?(vue)' --languageFiles './src/locales/**/*.?(json)' --exclude errors"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vitejs/plugin-vue": "^4.3.0",
    "@vue/runtime-core": "^3.3.4",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.4.4",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "vite": "^4.4.9",
    "vite-plugin-style-import": "^2.0.0",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.1",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.15",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.10",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.2",
    "consola": "^3.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "rollup": "^3.29.1",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.25.2",
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^1.8.10"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "types": {
        "build": {
          "description": "构建工具或外部依赖的改动"
        },
        "chore": {
          "description": "其他修改（不适用于已有 Type 的改动）"
        },
        "ci": {
          "description": "与 CI（持续集成服务）有关的改动"
        },
        "docs": {
          "description": "文档更新（documentation）"
        },
        "feat": {
          "description": "新特性或功能（feature）"
        },
        "fix": {
          "description": "缺陷修复"
        },
        "perf": {
          "description": "优化相关，如：提升性能、体验等"
        },
        "refactor": {
          "description": "代码重构，不引入新功能和缺陷修复"
        },
        "revert": {
          "description": "回滚到上一个版本，执行 git revert 打印的 message"
        },
        "release": {
          "description": "版本发布"
        },
        "style": {
          "description": "代码风格或者组件样式更新，不影响代码含义的改动，如：去掉空格、改变缩进、增删分号"
        },
        "test": {
          "description": "单元测试，新增测试或修改现有测试"
        },
        "ui": {
          "description": "CSS 样式相关改动"
        },
        "wip": {
          "description": "正在开发中尚未完成（work in process）"
        }
      }
    }
  }
}